<body>
  
    <p>
        <div class="album py-5 bg-light">
            <div class="container">
        
              <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
        <% for (var i = 0; i < records.length && i< records2.length; i++) { %>
            <div class="col">
                <div class="card shadow-sm">
                  <div class="imgSize">
                    <img src="images/<%= records[i]%>.jpg" style="height: 250px; width: 360px;" role="img" aria-label="Placeholder: Thumbnail"
                      preserveAspectRatio="xMidYMid slice" focusable="false">
                    </div>
                    <title>Placeholder</title>
                    <rect width="100%" height="100%" fill="#55595c" /><text x="50%" y="50%" fill="#eceeef"
                      dy=".3em"><%= records[i]%></text></svg>
                  <div class="card-body">
                    <p class="card-text">Description: <%= records2[i]%></p>
                    <p class="card-text">Units: <%= records3[i]%></p>
                    <p class="card-text">Weight: <%= records4[i]%></p>
                    <p class="card-text">Price: <%= records5[i]%></p>
                    <p class="card-text">Time: <%= records6[i]%></p>
                    <p class="card-text">Date: <%= records7[i]%></p>
                    <h5>COUNTDOWN</h5>
                        <p id="demo">
                        </p>
                    <div class="d-flex justify-content-between align-items-center">
                      <div class="btn-group">
                        
                        <button type="button" value="Edite" id="btn"
                          class="btn btn-sm btn-outline-secondary">Bid</button>
                        <!-- <input type="text" name="apple" style="visibility: hidden;"> -->
                      </div>
                      <!-- <small class="text-muted">9 mins</small> -->
                    </div>
                  </div>
                </div>
              </div>
        <% } %>
        </div>
        </div>
        </div>

</p>

<script>
    document.cookie = "name=abhi3423"
    // console.log(document.cookie);
    var dest=new Date("2023-01-22 20:51").getTime();
    var x = setInterval(() => {
    var now = new Date().getTime();
    var diff=dest-now;
    var days=Math.floor(diff/(1000*60*60*24));
 
    var hours = Math.floor((diff%(1000*60*60*24))/(1000*60*60));
  
    var minutes = Math.floor((diff%(1000*60*60))/(1000*60));

    var seconds = Math.floor((diff%(1000*60))/(1000));
   

    document.getElementById("demo").innerHTML=days +"d, "+hours+":"+minutes+":"+seconds
            
}, 1000);
</script>

</body>
